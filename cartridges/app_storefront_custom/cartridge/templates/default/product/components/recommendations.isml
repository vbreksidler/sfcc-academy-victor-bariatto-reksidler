<isloop items="${product.recommendedProducts.collection}" var="product">
    <div class="col-6 col-sm-4">
        <isobject object="${product.recommendedProducts.collection}" view="searchhit">
            <isinclude url="${URLUtils.url('Tile-Show', 'pid', product.ID, 'swatches', true, 'ratings', true)}" />
        </isobject>
    </div>
</isloop>